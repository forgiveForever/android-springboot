<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weibo.dao.MessageInfoDao">


    <!--通过Id查询Messageinfo-->
    <insert id="findMessageInfoById" parameterType="com.weibo.entity.MessageInfo"   >
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Long">
            select LAST_INSERT_ID()
        </selectKey>
        insert into user_t
        (

          ,
        user_name  ,
        pass_word  ,
        nick_name  ,
        birthday  ,
        hobby  ,
        native_place  ,
        address  ,
        qq  ,
        email  ,
        gender
        )
        values
        (

        #{phone_id} ,
        #{user_name} ,
        #{pass_word} ,
        #{nick_name} ,
        #{birthday} ,
        #{hobby} ,
        #{native_place} ,
        #{address} ,
        #{qq} ,
        #{email} ,
        #{gender}
        )
    </insert>



    <!-- 根据phone_id和pass_word 验证User账号 -->
    <select id="login" resultType="com.weibo.entity.User" parameterType="com.weibo.entity.User">
        select
        t0.id,
        t0.phone_id  ,
        t0.user_name  ,
        t0.pass_word  ,
        t0.nick_name  ,
        t0.birthday  ,
        t0.hobby  ,
        t0.native_place  ,
        t0.address  ,
        t0.qq  ,
        t0.email  ,
        t0.gender

        from user_t t0
            where t0.phone_id=#{phone_id}
        and   t0.pass_word=#{pass_word}
    </select>

    <!--编辑-->
    <update id="editUser"  parameterType="com.weibo.entity.User" >
        update user_t set
        phone_id=#{phone_id}  ,
        user_name=#{user_name}  ,
        pass_word=#{pass_word}  ,
        nick_name=#{nick_name}  ,
        birthday=#{birthday}  ,
        hobby=#{hobby}  ,
        native_place=#{native_place}  ,
        address=#{address}  ,
        qq=#{qq}  ,
        email=#{email}  ,
        gender=#{gender}
        where id=#{id}
    </update>



